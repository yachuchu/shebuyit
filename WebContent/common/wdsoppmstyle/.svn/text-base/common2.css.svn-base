@charset "utf-8";   /* CSS Document */
	/*================================================
 __Definition  of  Global style here    
 =================================================*/
body {
	width: 100%;
	overflow: hidden;
	font-family: Verdana, Arial, Helvetica, sans-serif
}

#wrapper {
	margin: 0 auto;
	width: 1250px;
	display: table;
}

.contentCenter {
	position: relative;
	width: 96%;
	margin: 0 auto;
	height: 100%;
	min-width: 96%;
}

/*================================================ 
__Definition  of  FOOTER style start here  
=================================================*/
#theFooter {
	width: 100%;
	margin: 0 auto;
	margin-top: 26px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
}

#footer_topBar {
	width: 100%;
	background: #494949;
	height: 3px;
	display: block;
}

#footer_content {
	font-size: 12px;
	line-height: 23px;
	clear: both;
	color: #666666;
	display: block;
	height: 25px;
	border-bottom: 1px solid #e5e5e5
}

#footer_content label {
	display: block;
	float: left;
	height: 23px;
}

#footer_links {
	float: right;
	line-height: 23px;
	font-size: 14px;
	color: #ccccc;
}

#footer_links a {
	font-size: 12px;
	line-height: 23px;
	color: #3678c2;
	text-decoration: none;
}

#footer_links a:hover {
	font-size: 12px;
	line-height: 23px;
	color: #Ef3A10;
	text-decoration: underline;
}

.footer_BlueChar {
	color: #3678C2;
	font-weight: bold
}

#footerHTML {
	background: #FFFFFF;
	position: absolute;
	left: 0px;
	bottom: 0px;
	color: #3678C2;
	font-size: 12px;
	width: 100%;
	text-align: center;
	height: 48px;
	border: 1px;
	border-style: solid;
	border-color: dedede;
	border-top: 2px solid #494949;
	line-height: 42px;
	z-index: 20
}

/*================================================ 
__Definition  of  MAIN_NAVIGATOR style start here  
=================================================*/
#mainNavigator {
	width: 100%; /*min-width: 100%; */
	font-size: 12px;
	color: #494949;
	height: 99px;
	top: 0px;
	left: 0px;
	float: left;
}  /*position:absolute; display:table; */
	/*#mainNaviLogoBar{ height:67px; }*/
#mainNaviLogoBar {
	height: 80px;
}

#mainNaviLogoBar span {
	width: 263px;
	float: left;
	text-align: left;
}  /* height:62px; margin-top:20px;*/
#mainNaviLogoBar label {
	float: right;
	font-size: 18px;
	margin-right: 15px;
	margin-top: 25px;
	line-height: 32px;
	font-weight: bold;
	color: #EF3A0F
}

#mainNaviContentoBar {
	height: 34px;
	width: 100%;
	min-width: min-width :   96%;
	background-image: url('../../images/naviBar_Repeat.gif');
	background-repeat: repeat-x repeat-y 2px;;
}

#mainNavi {
	cursor: pointer;
}

.mainNavi_span_span {
	padding-top: 2px;
	text-decoration: none;
	text-align: center;
	width: 140px;
	height: 32px;
	display: block;
	float: left;
	margin: 0 0 0 0;
	border: 0 0 0 0;
	font-size: 15;
	/*background-image: url("../../images/navi_hover.gif")*/
}

a:hover {
	color: blue;
}

#mainNaviContentoBar span {
	margin-left: 0px;
	line-height: 30px;
	height: 32px;
	font-weight: bold;
	float: left
}

#mainNaviContentoBar span a {
	color: #000000;
	text-decoration: none;
}

#mainNaviContentoBar span a:hover {
	color: #EF3A0F;
	text-decoration: none;
}

#mainNaviContentoBar label {
	line-height: 30px;
	display: block;
	float: left;
	color: #000000;
	margin-right: 12px;
}

.mainNaviContentoBar_label_a {
	color: white;
	text-decoration: none;
	padding-right: 5px;
}

#mainNaviContentoBar label em {
	font-style: normal;
	color: #F03A12;
}

.mainNaviContentoBar_label_em_a {
	color: white;
}

/*===================================================
Left Menu
******************************/ /*set the position of left menu, top*/
#left {
	min-height: 800px;
	text-align: left;
	display: block;
	width: 176px;
	height: 100%;
	position: absolute;
	top: 115px;
	padding-right: 4px;
	left: 0px;
	overflow: hidden;
	border: 1px;
	border-style: solid;
	border-color: dedede;
}

#leftMenu {
	width: 176px;
}

#leftMenu dl {
	margin: 0px;
	list-style-type: none;
}

#leftMenu dt {
	display: block;
	padding-left: 15px;
	height: 24px;
	line-height: 22px;
	font-size: 12px;
	list-style-type: none;
	cursor: pointer;
	margin: 0px;
	background: url('../../images/menu_Normal_176.jpg') no-repeat #dedede;
	font-family: Verdana, Arial, Helvetica, sans-serif;
}

#leftMenu dd {
	margin: 0px;
	display: block;
	padding-left: 25px;
	height: 24px;
	line-height: 24px;
	font-size: 12px;
	list-style-type: none;
	display: none;
	cursor: pointer;
	background-position: 200% 200%;
	font-family: Verdana, Arial, Helvetica, sans-serif;
}

iframe {
	margin: 0 0 0 0;
	width: 100%;
}

#contentBody {
	padding-left: 176px;
}  /*display:table;padding-top:112px;margin-top:0px;width: 100% */
	/*background:url(../../images/left_repeat.gif) 176px repeat-y; */
#rightContent {
	width: 1040px;
	float: left;
	display: tabel;
	height: 300px;
	display: table;
}

/***********
   Area Style
*************/
.staticArea {
	width: 100%;
}